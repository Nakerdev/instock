// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @id
  email String @unique
  password String
  name String
  surname String
  signUpDate DateTime
}

model Log {
  id Int @id @default(autoincrement())
  source String
  level String
  message String
  exception String
  createdAt DateTime
}